{
  "manifest_version": 3,
  "name": "Cookie Modifier Extension",
  "version": "1.0",
  "description": "Sets Secure=true and SameSite=None for all cookies",
  "permissions": [
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
